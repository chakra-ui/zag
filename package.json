{
  "name": "@zag-js/org",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chakra-ui/zag.git"
  },
  "author": "Segun Adebayo <sage@adebayosegun.com>",
  "license": "MIT",
  "scripts": {
    "build-fast": "turbo run build-fast --filter=./packages/**/*",
    "build": "turbo run build --filter=./packages/**/*",
    "start": "turbo run start --filter=./packages/**/*",
    "prepare": "husky install",
    "swc": "node -r @swc-node/register",
    "sync-pkgs": "pnpm swc scripts/sync-packages.ts",
    "clean-pkgs": "pnpm -r exec rm -rf dist .turbo .swc *.log",
    "clean": "pnpm clean-pkgs && rm -rf node_modules",
    "react": "pnpm --filter \"./examples/next-ts\"",
    "vue": "pnpm --filter \"./examples/vue-ts\"",
    "solid": "pnpm --filter \"./examples/solid-ts\"",
    "start-react": "pnpm react dev",
    "start-vue": "pnpm vue dev",
    "start-solid": "pnpm solid dev",
    "pw-test": "playwright test",
    "pw-test-debug": "PWDEBUG=1 playwright test",
    "e2e-react": "start-server-and-test \"pnpm react test-dev\" http://localhost:3000 \"PORT=3000 pnpm pw-test\"",
    "e2e-vue": "start-server-and-test \"pnpm vue test-dev\" http-get://localhost:3001 \"PORT=3001 pnpm pw-test\"",
    "e2e-solid": "PORT=3002 start-server-and-test \"pnpm solid test-dev\" http-get://localhost:3002 \"PORT=3002 pnpm pw-test\"",
    "generate-machine": "plop machine && pnpm sync-pkgs",
    "generate-util": "plop utility && pnpm sync-pkgs",
    "typecheck": "turbo run typecheck --filter=./packages/**/*",
    "prettier": "prettier --check packages/**/*",
    "prettier-fix": "prettier --write packages/**/*",
    "lint": "eslint packages --ext .ts",
    "version": "changeset version",
    "release": "changeset publish",
    "test": "jest",
    "visualize": "pnpm swc scripts/visualize.ts --all",
    "changelog": "pnpm swc scripts/changelog.ts"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": "prettier --write",
    "**/*.machine.ts": [
      "pnpm visualize",
      "git add .xstate/*.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@axe-core/playwright": "4.4.4",
    "@babel/generator": "7.18.13",
    "@babel/parser": "7.18.13",
    "@babel/traverse": "7.18.13",
    "@babel/types": "7.18.13",
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "2.24.4",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@octokit/rest": "19.0.4",
    "@playwright/test": "1.25.1",
    "@swc-node/jest": "1.5.2",
    "@swc-node/register": "1.5.1",
    "@types/jest": "28.1.8",
    "@types/signale": "1.4.4",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "axe-core": "4.4.3",
    "commitlint": "17.0.3",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "find-packages": "9.0.9",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-watch-typeahead": "2.1.1",
    "json-format-highlight": "1.0.4",
    "lint-staged": "13.0.3",
    "playwright": "1.25.1",
    "plop": "3.1.1",
    "prettier": "2.7.1",
    "start-server-and-test": "1.14.0",
    "signale": "1.4.0",
    "turbo": "1.4.3",
    "tsup": "6.2.3",
    "typescript": "4.7.4"
  }
}
